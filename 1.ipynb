    "\n",
    "import numpy as np\n",
    "t = np.array([[1, 2, 3], [4, 5, 6]])\n",
    "t1 = t * 2 \n",
    "print(\"\\n\",t1)\n",
    "#Element-wise addition\n",
    "t2 = t + 3\n",
    "print(\"\\n\", t2)\n",
    "# Element-wise exponentiation \n",
    "t3 = np.power(t, 2)\n",
    "print(\" \\n\",t3)\n",
    "# Transpose\n",
    "t4 = np.transpose(t)\n",
    "print(\"\\n\",t4)"
   ]
